{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
       "linebreak-style": "off",
       "indent": "off",
       "require-jsdoc": "off",
       "comma-dangle": "off",
       "object-curly-spacing": "off"
    },
   "extends": ["eslint:recommended", "google"]
}